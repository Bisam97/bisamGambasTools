' Gambas class file

Private Set As New Settings
Public Sub Form_Open()
Versluesselt.Text = Set["ssh/PW", ""]
ssh_User.Text = Set["ssh/User", ""]
ssh_IP.Text = Set["ssh/IP", "0.0.0.0"]
If Versluesselt Not "" Then
  ssh_PW.Text = Pwg8.DecodePW(Versluesselt.Text)
  
Endif
  
End






Public Sub save_Click()

Fsetings.Versluesselt.Text = Pwg8.EncodePW(Fsetings.ssh_PW.Text)  
Set["ssh/PW"] = Versluesselt.Text
Set["ssh/User"] = Fsetings.ssh_User.Text
Set["ssh/IP"] = Fsetings.ssh_IP.Text
Set.Save
Me.Close
End


 

Public Sub Button1_Click()

  FadUserpiRS.Show

End
